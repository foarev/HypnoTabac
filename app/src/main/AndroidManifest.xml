<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hypnotabac">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".hypno.AddClientActivity"></activity>
        <activity android:name=".hypno.HypnoMainActivity" />
        <activity android:name=".hypno.HypnoSignupActivity" />
        <activity android:name=".FirstLoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".hypno.HypnoLoginActivity" />
        <activity android:name=".client.ClientMainActivity"/>
        <activity android:name=".client.ClientLoginActivity"/>
        <activity android:name=".client.ClientSignupActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <data
                    android:scheme="https"
                    android:host="hypnotabac-1c267.firebaseapp.com"/>
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>